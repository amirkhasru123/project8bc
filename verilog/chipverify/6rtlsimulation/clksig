v0x5a4daf12a730_0 .var "clk", 0 0;
v0x5a4daf172630_0 .var "signal", 0 0;
    .scope S_0x5a4daf12a5a0;
T_0 ;
    %delay 5000, 0;
    %load/vec4 v0x5a4daf12a730_0;
    %inv;
    %store/vec4 v0x5a4daf12a730_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5a4daf12a5a0;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "clksig" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a4daf12a5a0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a4daf12a730_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a4daf172630_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a4daf172630_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a4daf172630_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a4daf172630_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a4daf172630_0, 0, 1;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_1;
