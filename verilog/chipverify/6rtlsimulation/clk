#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b528539e5a0 .scope module, "clk" "clk" 2 3;
 .timescale -9 -12;
v0x5b528539e730_0 .var "clk", 0 0;
    .scope S_0x5b528539e5a0;
T_0 ;
    %delay 5000, 0;
    %load/vec4 v0x5b528539e730_0;
    %inv;
    %store/vec4 v0x5b528539e730_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b528539e5a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b528539e730_0, 0, 1;
    %vpi_call 2 10 "$dumpfile", "clk.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, v0x5b528539e730_0 {0 0 0};
    %delay 30000, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "clk.v";
