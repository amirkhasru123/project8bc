#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6036ede0e400 .scope module, "test" "test" 2 30;
 .timescale 0 0;
v0x6036ede20fa0_0 .var "clk", 0 0;
v0x6036ede21060_0 .net "out", 2 0, v0x6036ede20c80_0;  1 drivers
v0x6036ede21130_0 .var "rstn", 0 0;
v0x6036ede21230_0 .var "up_down", 0 0;
S_0x6036ede0e590 .scope module, "dut" "counter" 2 37, 2 6 0, S_0x6036ede0e400;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "up_down";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rstn";
    .port_info 3 /OUTPUT 3 "out";
v0x6036eddd4490_0 .net "clk", 0 0, v0x6036ede20fa0_0;  1 drivers
v0x6036ede20c80_0 .var "out", 2 0;
v0x6036ede20d60_0 .net "rstn", 0 0, v0x6036ede21130_0;  1 drivers
v0x6036ede20e30_0 .net "up_down", 0 0, v0x6036ede21230_0;  1 drivers
E_0x6036ede0d2e0/0 .event negedge, v0x6036ede20d60_0;
E_0x6036ede0d2e0/1 .event posedge, v0x6036eddd4490_0;
E_0x6036ede0d2e0 .event/or E_0x6036ede0d2e0/0, E_0x6036ede0d2e0/1;
    .scope S_0x6036ede0e590;
T_0 ;
    %wait E_0x6036ede0d2e0;
    %load/vec4 v0x6036ede20d60_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x6036ede20c80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6036ede20e30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x6036ede20c80_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x6036ede20c80_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x6036ede20c80_0;
    %subi 1, 0, 3;
    %assign/vec4 v0x6036ede20c80_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6036ede0e400;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6036ede20fa0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x6036ede0e400;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x6036ede20fa0_0;
    %inv;
    %store/vec4 v0x6036ede20fa0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6036ede0e400;
T_3 ;
    %vpi_call 2 49 "$monitor", "Time=%0t up_down=%b clk=%b rstn=%b out=%b", $time, v0x6036ede21230_0, v0x6036ede20fa0_0, v0x6036ede21130_0, v0x6036ede21060_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6036ede21130_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6036ede21230_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6036ede21130_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 58 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "counter.v";
