#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x582f4f304a30 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
E_0x582f4f2dea00 .event "b_event3";
E_0x582f4f2deab0 .event "a_event";
    .scope S_0x582f4f304a30;
T_0 ;
    %delay 20, 0;
    %event E_0x582f4f2deab0;
    %delay 30, 0;
    %event E_0x582f4f2deab0;
    %delay 50, 0;
    %event E_0x582f4f2deab0;
    %delay 10, 0;
    %event E_0x582f4f2dea00;
    %end;
    .thread T_0;
    .scope S_0x582f4f304a30;
T_1 ;
    %wait E_0x582f4f2deab0;
    %vpi_call 2 13 "$display", "T=%0t [always] a_event is triggered", $time {0 0 0};
    %jmp T_1;
    .thread T_1;
    .scope S_0x582f4f304a30;
T_2 ;
    %delay 25, 0;
    %wait E_0x582f4f2deab0;
    %vpi_call 2 18 "$display", "T=%0t [initial] a_event is triggered", $time {0 0 0};
    %delay 10, 0;
    %wait E_0x582f4f2dea00;
    %vpi_call 2 22 "$display", "T=%0t [initial] b_event3 is triggered", $time {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "namedevents.v";
