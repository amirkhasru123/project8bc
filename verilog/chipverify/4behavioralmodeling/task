#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58861a013a50 .scope module, "taskEx" "taskEx" 2 2;
 .timescale 0 0;
S_0x588619fda490 .scope task, "display" "display" 2 8, 2 8 0, S_0x58861a013a50;
 .timescale 0 0;
v0x588619fda670_0 .var/i "i", 31 0;
TD_taskEx.display ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x588619fda670_0, 0, 32;
    %load/vec4 v0x588619fda670_0;
    %addi 1, 0, 32;
    %store/vec4 v0x588619fda670_0, 0, 32;
    %vpi_call 2 14 "$display", "i=%0d", v0x588619fda670_0 {0 0 0};
    %end;
S_0x58861a013be0 .scope module, "taskWithTimeControl" "taskWithTimeControl" 2 19;
 .timescale 0 0;
v0x58861a025150_0 .var/i "i", 31 0;
S_0x58861a024d50 .scope task, "display" "display" 2 29, 2 29 0, S_0x58861a013be0;
 .timescale 0 0;
TD_taskWithTimeControl.display ;
    %delay 5, 0;
    %fork TD_taskWithTimeControl.incre, S_0x58861a024f50;
    %join;
    %delay 5, 0;
    %vpi_call 2 32 "$display", "i=%0d", v0x58861a025150_0 {0 0 0};
    %end;
S_0x58861a024f50 .scope task, "incre" "incre" 2 36, 2 36 0, S_0x58861a013be0;
 .timescale 0 0;
TD_taskWithTimeControl.incre ;
    %load/vec4 v0x58861a025150_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x58861a025150_0, 0;
    %end;
    .scope S_0x58861a013a50;
T_3 ;
    %fork TD_taskEx.display, S_0x588619fda490;
    %join;
    %end;
    .thread T_3;
    .scope S_0x58861a013a50;
T_4 ;
    %fork TD_taskEx.display, S_0x588619fda490;
    %join;
    %end;
    .thread T_4;
    .scope S_0x58861a013a50;
T_5 ;
    %fork TD_taskEx.display, S_0x588619fda490;
    %join;
    %end;
    .thread T_5;
    .scope S_0x58861a013a50;
T_6 ;
    %fork TD_taskEx.display, S_0x588619fda490;
    %join;
    %end;
    .thread T_6;
    .scope S_0x58861a013be0;
T_7 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58861a025150_0, 0, 32;
    %end;
    .thread T_7;
    .scope S_0x58861a013be0;
T_8 ;
    %fork TD_taskWithTimeControl.display, S_0x58861a024d50;
    %join;
    %delay 1, 0;
    %fork TD_taskWithTimeControl.display, S_0x58861a024d50;
    %join;
    %delay 1, 0;
    %fork TD_taskWithTimeControl.display, S_0x58861a024d50;
    %join;
    %delay 1, 0;
    %fork TD_taskWithTimeControl.display, S_0x58861a024d50;
    %join;
    %end;
    .thread T_8;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "task.v";
