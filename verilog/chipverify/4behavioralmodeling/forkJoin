#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61356ea01560 .scope module, "forkJoin" "forkJoin" 2 1;
 .timescale 0 0;
v0x61356ea016f0_0 .var "data", 7 0;
    .scope S_0x61356ea01560;
T_0 ;
    %delay 10, 0;
    %pushi/vec4 254, 0, 8;
    %store/vec4 v0x61356ea016f0_0, 0, 8;
    %vpi_call 2 6 "$display", "Before forking %h", v0x61356ea016f0_0 {0 0 0};
    %fork t_1, S_0x61356ea01560;
    %fork t_2, S_0x61356ea01560;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %delay 20, 0;
    %pushi/vec4 17, 0, 8;
    %store/vec4 v0x61356ea016f0_0, 0, 8;
    %end;
t_2 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x61356ea016f0_0, 0, 8;
    %end;
    .scope S_0x61356ea01560;
t_0 ;
    %vpi_call 2 11 "$display", "After forking and joining %h", v0x61356ea016f0_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
