#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e90f95e5510 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x5e90f95e5750_0 .net *"_ivl_5", 0 0, L_0x5e90f96307e0;  1 drivers
v0x5e90f9630680_0 .var "a", 0 0;
v0x5e90f9630740_0 .var "b", 0 0;
E_0x5e90f95e5a30 .event anyedge, v0x5e90f9630680_0;
E_0x5e90f95e5910 .event posedge, L_0x5e90f96307e0;
E_0x5e90f95e5a70 .event posedge, v0x5e90f9630740_0;
E_0x5e90f96091d0 .event posedge, v0x5e90f9630680_0;
L_0x5e90f96307e0 .arith/sum 1, v0x5e90f9630680_0, v0x5e90f9630740_0;
    .scope S_0x5e90f95e5510;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5e90f9630680_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5e90f9630680_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5e90f9630740_0, 0;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5e90f9630680_0, 0;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5e90f9630680_0, 0;
    %end;
    .thread T_0;
    .scope S_0x5e90f95e5510;
T_1 ;
    %wait E_0x5e90f96091d0;
    %vpi_call 2 19 "$display", "T=%0t Posedge of a detected for 0->1", $time {0 0 0};
    %wait E_0x5e90f95e5a70;
    %vpi_call 2 21 "$display", "T=%0t Posedge of b detected for X->1", $time {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5e90f95e5510;
T_2 ;
    %wait E_0x5e90f95e5910;
    %vpi_call 2 25 "$display", "T=%0t Posedge of a+b", $time {0 0 0};
    %wait E_0x5e90f95e5a30;
    %vpi_call 2 27 "$display", "T=%0t Change in a found", $time {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "eventcontrol.v";
