#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x646efa6d0de0 .scope module, "ifelseEx" "ifelseEx" 2 26;
 .timescale 0 0;
v0x646efa6cfc30_0 .var "a", 0 0;
S_0x646efa6d0f70 .scope module, "test" "test" 2 15;
 .timescale 0 0;
v0x646efa6e26c0_0 .var "a", 0 0;
v0x646efa6e2790_0 .var "b", 0 0;
v0x646efa6e2860_0 .net "x", 0 0, v0x646efa6e25a0_0;  1 drivers
S_0x646efa6e2150 .scope module, "dut" "controlblock" 2 19, 2 1 0, S_0x646efa6d0f70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "x";
v0x646efa6e2400_0 .net "a", 0 0, v0x646efa6e26c0_0;  1 drivers
v0x646efa6e24e0_0 .net "b", 0 0, v0x646efa6e2790_0;  1 drivers
v0x646efa6e25a0_0 .var "x", 0 0;
E_0x646efa695b60 .event anyedge, v0x646efa6e2400_0, v0x646efa6e24e0_0;
    .scope S_0x646efa6d0de0;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x646efa6cfc30_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x646efa6d0de0;
T_1 ;
    %delay 20, 0;
    %load/vec4 v0x646efa6cfc30_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 30 "$display", "a is 1" {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 33 "$display", "a is not 1" {0 0 0};
T_1.1 ;
    %end;
    .thread T_1;
    .scope S_0x646efa6e2150;
T_2 ;
    %wait E_0x646efa695b60;
    %load/vec4 v0x646efa6e2400_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_2.2, 4;
    %load/vec4 v0x646efa6e24e0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_2.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x646efa6e25a0_0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x646efa6e2400_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_2.5, 4;
    %load/vec4 v0x646efa6e24e0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_2.5;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.3, 8;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x646efa6e25a0_0;
    %jmp T_2.4;
T_2.3 ;
    %load/vec4 v0x646efa6e2400_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_2.8, 4;
    %load/vec4 v0x646efa6e24e0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_2.8;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.6, 8;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x646efa6e25a0_0;
    %jmp T_2.7;
T_2.6 ;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x646efa6e25a0_0;
T_2.7 ;
T_2.4 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x646efa6d0f70;
T_3 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x646efa6e26c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x646efa6e2790_0, 0, 1;
    %delay 3, 0;
    %vpi_call 2 22 "$display", "%b %b = %b", v0x646efa6e26c0_0, v0x646efa6e2790_0, v0x646efa6e2860_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
