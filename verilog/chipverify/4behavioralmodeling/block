#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59fdbe7f7d20 .scope module, "blockEx" "blockEx" 2 1;
 .timescale 0 0;
v0x59fdbe7f8060_0 .var "a", 0 0;
v0x59fdbe809c90_0 .var "b", 0 0;
v0x59fdbe809d50_0 .var "c", 0 0;
S_0x59fdbe7f7eb0 .scope module, "notBlock" "notBlock" 2 11;
 .timescale 0 0;
v0x59fdbe809df0_0 .var "a", 0 0;
v0x59fdbe809ed0_0 .var "b", 0 0;
v0x59fdbe809f90_0 .var "c", 0 0;
    .scope S_0x59fdbe7f7d20;
T_0 ;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59fdbe7f8060_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59fdbe809c90_0, 0, 1;
    %vpi_call 2 6 "$display", "a=%b, b=%b got assigned, but c=%b is not assigned", v0x59fdbe7f8060_0, v0x59fdbe809c90_0, v0x59fdbe809d50_0 {0 0 0};
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59fdbe809d50_0, 0, 1;
    %vpi_call 2 7 "$display", "a=%b, b=%b, c=%b is now assigned", v0x59fdbe7f8060_0, v0x59fdbe809c90_0, v0x59fdbe809d50_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x59fdbe7f7eb0;
T_1 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x59fdbe809df0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x59fdbe809ed0_0, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59fdbe809f90_0, 0, 1;
    %vpi_call 2 19 "$display", "a=%b, b=%b are not assigned yet, but c=%b got assigned", v0x59fdbe809df0_0, v0x59fdbe809ed0_0, v0x59fdbe809f90_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 21 "$display", "After time step a=%b, b=%b, c=%b all are assigned", v0x59fdbe809df0_0, v0x59fdbe809ed0_0, v0x59fdbe809f90_0 {0 0 0};
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "block.v";
