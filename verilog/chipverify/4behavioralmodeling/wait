#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5bcc4af2d570 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x5bcc4af51fc0_0 .var "clk", 0 0;
v0x5bcc4af76df0_0 .var "ctr", 3 0;
E_0x5bcc4af2e6c0 .event posedge, v0x5bcc4af51fc0_0;
E_0x5bcc4af2dc10 .event anyedge, v0x5bcc4af76df0_0;
    .scope S_0x5bcc4af2d570;
T_0 ;
    %pushi/vec4 0, 0, 5;
    %split/vec4 1;
    %assign/vec4 v0x5bcc4af51fc0_0, 0;
    %assign/vec4 v0x5bcc4af76df0_0, 0;
T_0.0 ;
    %load/vec4 v0x5bcc4af76df0_0;
    %or/r;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.1, 6;
    %wait E_0x5bcc4af2dc10;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 9 "$display", "T=%0t Counter reached non-zero value 0x%0h", $time, v0x5bcc4af76df0_0 {0 0 0};
T_0.2 ;
    %load/vec4 v0x5bcc4af76df0_0;
    %pad/u 32;
    %pushi/vec4 4, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.3, 6;
    %wait E_0x5bcc4af2dc10;
    %jmp T_0.2;
T_0.3 ;
    %vpi_call 2 11 "$display", "T=%0t Counter reached 0x%0h", $time, v0x5bcc4af76df0_0 {0 0 0};
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5bcc4af2d570;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x5bcc4af51fc0_0;
    %inv;
    %store/vec4 v0x5bcc4af51fc0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5bcc4af2d570;
T_2 ;
    %wait E_0x5bcc4af2e6c0;
    %load/vec4 v0x5bcc4af76df0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5bcc4af76df0_0, 0;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "wait.v";
