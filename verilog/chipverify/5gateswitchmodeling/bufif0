:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58ef34e07320 .scope module, "tb_bufif1_example" "tb_bufif1_example" 2 7;
 .timescale -9 -12;
v0x58ef34e18ec0_0 .var "a", 0 0;
v0x58ef34e18f90_0 .var "en", 0 0;
v0x58ef34e19060_0 .net8 "y", 0 0, L_0x58ef34e19160;  1 drivers, strength-aware
S_0x58ef34e074b0 .scope module, "uut" "bufif0_example" 2 12, 2 3 0, S_0x58ef34e07320;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "en";
    .port_info 2 /OUTPUT 1 "y";
L_0x58ef34e19160 .functor BUFIF0 1, v0x58ef34e18ec0_0, v0x58ef34e18f90_0, C4<0>, C4<0>;
v0x58ef34dce490_0 .net "a", 0 0, v0x58ef34e18ec0_0;  1 drivers
v0x58ef34e18ce0_0 .net "en", 0 0, v0x58ef34e18f90_0;  1 drivers
v0x58ef34e18da0_0 .net8 "y", 0 0, L_0x58ef34e19160;  alias, 1 drivers, strength-aware
    .scope S_0x58ef34e07320;
T_0 ;
    %vpi_call 2 15 "$monitor", "%g\011  %b  %b | %b", $time, v0x58ef34e18f90_0, v0x58ef34e18ec0_0, v0x58ef34e19060_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ef34e18f90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ef34e18ec0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58ef34e18f90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ef34e18ec0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58ef34e18f90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58ef34e18ec0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ef34e18f90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58ef34e18ec0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
