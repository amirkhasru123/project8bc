S_0x55b8da989c50 .scope module, "tb" "tb" 2 11;
 .timescale 0 0;
v0x55b8da99be30_0 .var "en", 0 0;
v0x55b8da99bf00_0 .var/i "i", 31 0;
v0x55b8da99bfc0_0 .var "in", 3 0;
v0x55b8da99c0c0_0 .net "out", 15 0, v0x55b8da99bcf0_0;  1 drivers
S_0x55b8da989de0 .scope module, "u0" "dec_3x8" 2 17, 2 1 0, S_0x55b8da989c50;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "en";
    .port_info 1 /INPUT 4 "in";
    .port_info 2 /OUTPUT 16 "out";
v0x55b8da989fc0_0 .net "en", 0 0, v0x55b8da99be30_0;  1 drivers
v0x55b8da99bc10_0 .net "in", 3 0, v0x55b8da99bfc0_0;  1 drivers
v0x55b8da99bcf0_0 .var "out", 15 0;
E_0x55b8da951670 .event anyedge, v0x55b8da99bc10_0, v0x55b8da989fc0_0;
    .scope S_0x55b8da989de0;
T_0 ;
    %wait E_0x55b8da951670;
    %load/vec4 v0x55b8da989fc0_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %pushi/vec4 1, 0, 16;
    %ix/getv 4, v0x55b8da99bc10_0;
    %shiftl 4;
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %pushi/vec4 0, 0, 16;
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %store/vec4 v0x55b8da99bcf0_0, 0, 16;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b8da989c50;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b8da99be30_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55b8da99bfc0_0, 0;
    %vpi_call 2 23 "$monitor", "en=%0b in=0x%0h out=0x%0h", v0x55b8da99be30_0, v0x55b8da99bfc0_0, v0x55b8da99c0c0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55b8da99bf00_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x55b8da99bf00_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x55b8da99bf00_0;
    %pad/s 5;
    %split/vec4 4;
    %store/vec4 v0x55b8da99bfc0_0, 0, 4;
    %store/vec4 v0x55b8da99be30_0, 0, 1;
    %delay 10, 0;
    %load/vec4 v0x55b8da99bf00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55b8da99bf00_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
