#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59aa7ff91530 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
    .scope S_0x59aa7ff91530;
T_0 ;
    %vpi_func 2 3 "$test$plusargs" 32, "STANDBY" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 4 "$display", "STANDBY argument is found ..." {0 0 0};
T_0.0 ;
    %vpi_func 2 6 "$test$plusargs" 32, "Standby" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %vpi_call 2 7 "$display", "Standby argument is also found ..." {0 0 0};
T_0.2 ;
    %vpi_func 2 9 "$test$plusargs" 32, "STAND" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %vpi_call 2 10 "$display", "STAND substring is found ..." {0 0 0};
T_0.4 ;
    %vpi_func 2 12 "$test$plusargs" 32, "S" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %vpi_call 2 13 "$display", "Some string starting with S found ..." {0 0 0};
T_0.6 ;
    %vpi_func 2 15 "$test$plusargs" 32, "T" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.8, 4;
    %vpi_call 2 16 "$display", "Some string containing T found ..." {0 0 0};
T_0.8 ;
    %vpi_func 2 18 "$test$plusargs" 32, "STAND_AT_EASE" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.10, 4;
    %vpi_call 2 19 "$display", "Can't stand any longer ..." {0 0 0};
T_0.10 ;
    %vpi_func 2 21 "$test$plusargs" 32, "SUNSHADE" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.12, 4;
    %vpi_call 2 22 "$display", "It's too hot today ..." {0 0 0};
T_0.12 ;
    %vpi_func 2 24 "$test$plusargs" 32, "WINTER" {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.14, 4;
    %vpi_call 2 25 "$display", "No match.. " {0 0 0};
T_0.14 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
